{
    "compilerOptions": {
      "target": "es2020",
      "module": "esnext",
      "moduleResolution": "node",
      "jsx": "react-jsx",
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "baseUrl": ".",
      "paths": {
        "@/*": ["resources/js/*"]
      },
      "types": ["vite/client"]
    },
    "include": ["resources/js/**/*"],
    "exclude": ["node_modules"],


}
